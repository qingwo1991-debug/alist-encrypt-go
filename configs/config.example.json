{
  "scheme": {
    "address": "0.0.0.0",
    "http_port": 5344,
    "https_port": -1,
    "force_https": false,
    "cert_file": "",
    "key_file": "",
    "unix_file": "",
    "unix_file_perm": "",
    "enable_h2c": false
  },
  "alist": {
    "host": "localhost",
    "port": 5244,
    "https": false
  },
  "cache": {
    "enable": true,
    "expiration": 10,
    "cleanup_interval": 5
  },
  "proxy": {
    "max_idle_conns": 100,
    "max_idle_conns_per_host": 100,
    "max_conns_per_host": 100,
    "idle_conn_timeout": 90,
    "enable_http2": true,
    "insecure_skip_verify": false
  },
  "log": {
    "level": "info",
    "format": "console",
    "output": "stdout"
  },
  "data_dir": "./data",
  "jwt_secret": "change-this-to-a-secure-random-string",
  "jwt_expire": 24
}
